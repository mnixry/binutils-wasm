{
  "name": "@binutils-wasm/binutils",
  "version": "0.1.0",
  "description": "GNU/Binutils compiled to WebAssembly",
  "repository": {
    "type": "git",
    "url": "https://github.com/mnixry/binutils-wasm.git",
    "directory": "packages/binutils"
  },
  "homepage": "https://github.com/mnixry/binutils-wasm#readme",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:wrapper": "tsup",
    "build:wasm": "docker buildx build --progress=plain --build-arg BRANCH=binutils-2_41-branch --output . ./build",
    "build": "pnpm run build:wrapper && pnpm run build:wasm",
    "prebuild": "rm -rf ./dist"
  },
